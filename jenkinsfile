pipeline{
	agent {
	label 'label1'
	}
	stages {
  stage('BUILD') {
    steps {
     echo "this is build stage"
	 sh 'sleep 5'
    }
  }

  stage('TEST') {
    steps {
      echo "this is test stage"
	  sh 'sleep 5'
    }
  }

  stage('DEPLOY') {
    steps {
      echo "this is test stage"
	  sh 'sleep 5'
    }
  }

}
	
}