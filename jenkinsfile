  stage('DEPLOY') {
    steps {
      echo "this is deploy stage"
	  sh 'sleep 5'
    }
  }
  
    }
  }

stage('TEST') {

    steps {
      echo "this is test stage"
	  echo "$NAME"
	  sh 'sleep 5'
    }
  }

}
